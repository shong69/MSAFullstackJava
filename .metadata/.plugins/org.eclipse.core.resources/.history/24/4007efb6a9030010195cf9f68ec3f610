package com.am;

abstract class Ab06{
	abstract void show();
}
interface Inter06{}

public class Ex06 {
	public static void main(String[] args) {
		Object obj=new Object() {
			@Override
			public String toString() {
				return "object를 상속받은 이름없는 클래스";
			}
		};

		System.out.println(obj);
		
		Ab06 obj2=new Ab06() {};
		Inter06 obj3=new Inter06() {};
		
		Ab06[] arr1= {
				new Ab06() {int su = 1111;}
				,new Ab06() {int su2 = 2222;}
		};
	}
	
}
