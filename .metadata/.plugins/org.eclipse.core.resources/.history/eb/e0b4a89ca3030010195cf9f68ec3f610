package com.am;
class Lec05{
	
	static Local func01() {
		class Local{
			void func02() {
				System.out.println("내부 클래스의 기능 수행");
			}
		}
		Local local = new Local();
//		local.func02();
		return new Local();
	}
}
public class Ex05 {
	public static void main(String[] args) {
		//익명 클래스
		Lec05.func01();
		local.func02();
	}
}
