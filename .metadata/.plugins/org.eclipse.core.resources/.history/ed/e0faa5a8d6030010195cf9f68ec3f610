package com.pm;

public class Ex08 {
	public static void main(String[] args) {
		int[] lotto = new int[45];
		for(int i = 0;i<lotto.length;i++) {
			lotto[i] = i+1;
		}
		//¼ýÀÚ ¼¯±â
		for(int i = 0;i<1000;i++) {
			int ran =(int)(Math.random()*45)+1;
			int temp = lotto[0];
			lotto[0] = lotto[ran];
			lotto[ran] = temp;
		}

		for (int i = 0; i < 6; i++) {
			System.out.println(lotto[i]);
		}
	}
}
